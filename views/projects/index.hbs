<h1>{{title}}</h1>
<a href="/projects/add">Add a New Project</a>

<table class="table table-striped table-hover">
    <thead>
    <th>Name</th>
    <th>Phone Number</th>
    <th>Email</th>
    {{#each projects}}
        <tr>
            <td>{{this.name}}</td>
            <td>{{this.phone}}</td>
            <td>{{this.email}}</td>
        </tr>
    {{/each}}
    </thead>
</table>